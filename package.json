    "@zerollup/ts-transform-paths": "^1.7.18",
    "bootstrap": "^5.1.1",
    "bootstrap-icons": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.18.0",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
